            <Insets />
         </opaqueInsets>
      </Label>
      <Label fx:id="Balance" alignment="CENTER" layoutX="494.0" layoutY="128.0" prefHeight="20.0" prefWidth="64.0" text="Balance" textFill="WHITE">
