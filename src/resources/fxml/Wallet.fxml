            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <FontAwesomeIconView fx:id="user_icon" fill="#acacac" glyphName="USER" layoutX="95.0" layoutY="63.0" onMouseClicked="#user_iconPressed" size="30" />
      <JFXButton fx:id="Transfer" layoutX="892.0" layoutY="46.0" onMouseClicked="#transferClicked" prefHeight="44.0" prefWidth="113.0" style="-fx-background-color: #1F98F5;" text="Transfer" textFill="WHITE">
