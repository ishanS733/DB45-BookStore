<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextArea?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: #2D3447;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.helpController">
    <children>
        <VBox alignment="CENTER" prefHeight="800.0" prefWidth="250.0" spacing="20.0" AnchorPane.topAnchor="0.0">
            <children>
                <JFXButton fx:id="home" onMouseClicked="#homePressed" prefHeight="37.0" prefWidth="133.0" text="Home" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </JFXButton>
            </children>
        </VBox>
      <JFXTextArea fx:id="queryPanel" focusColor="#2b3054" layoutX="365.0" layoutY="90.0" prefHeight="191.0" prefWidth="692.0" promptText="Your Query Here" style="-fx-prompt-text-fill: WHITE; -fx-text-fill: WHITE;" unFocusColor="#2b3054">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></JFXTextArea>
      <Label layoutX="365.0" layoutY="48.0" text="Enter You Queries Here" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <JFXButton fx:id="submitButton" layoutX="655.0" layoutY="301.0" onMouseClicked="#submitButtonPressed" prefHeight="37.0" prefWidth="133.0" text="Submit" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </JFXButton>
      <TableView fx:id="previousQueriesTable" layoutX="382.0" layoutY="418.0" prefHeight="344.0" prefWidth="710.0" />
      <Label layoutX="383.0" layoutY="375.0" text="Your Previous Queries" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
