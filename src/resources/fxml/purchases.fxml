         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <padding>
            <Insets top="100.0" />
         </padding>
      </Label>
      <Label fx:id="userWelcomeLabel" layoutX="48.0" layoutY="82.0" prefHeight="39.0" prefWidth="204.0" text="Label" textFill="WHITE">
